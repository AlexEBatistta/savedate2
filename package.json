    "@pixi/ui": "^0.7.2",
    "@types/node": "^20.6.3",
    "bigscreen": "^2.0.5",
    "firebase": "^10.4.0",
    "firebase-admin": "^12.0.0",
    "googleapis": "^126.0.1",
    "hammerjs": "^2.0.8",
    "i18next": "22.4.14",
    "localforage": "1.10.0",
    "ml-xsadd": "^2.0.0",
    "papaparse": "^5.4.1",
    "photoswipe": "^5.4.3",
    "pixi.js": "^7.2.4",
    "screenfull": "^6.0.2",
    "stats.js": "^0.17.0",
    "ts-node": "^10.9.1",
    "util": "^0.12.5"
  }
