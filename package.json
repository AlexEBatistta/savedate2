{
  "name": "SaveDate",
  "version": "0.0.1",
  "description": "BunnyBox is a boilerplate/engine for pixijs. Descendant of the great killabunnies engine",
  "pwa": {
    "name": "BunnyBox powered PWA",
    "short_name": "BunnyBox",
    "description": "My awesome Progressive Web App!",
    "background_color": "#ee0000"
  },
  "main": "index.js",
  "scripts": {
    "build:release": "rimraf ./dist && webpack --config webpack.config.prod.js",
    "build:debug": "rimraf ./dist && webpack --config webpack.config.dev.js",
    "start": "webpack serve --config webpack.config.dev.js",
    "start:release": "webpack serve --config webpack.config.prod.js",
    "lint": "eslint src/**/*.ts --fix",
    "clean": "rimraf ./dist",
    "check": "tsc --noEmit --skipLibCheck -p tsconfig.json",
    "deploy": "npm run build:release && gh-pages -d dist"
  },
  "languages": [
    "en",
    "pt",
    "es"
  ],
  "author": "ElementalCode (Milton Candelero) @ Killabunnies",
  "devDependencies": {
    "@killabunnies/eslint-config": "^1.0.5",
    "@types/file-saver": "^2.0.5",
    "@types/fontfaceobserver": "2.1.0",
    "@types/hammerjs": "^2.0.41",
    "@types/papaparse": "^5.3.8",
    "@types/sat": "^0.0.31",
    "@types/stats.js": "^0.17.0",
    "@types/webpack-env": "^1.17.0",
    "copy-webpack-plugin": "11.0.0",
    "esbuild-loader": "3.0.1",
    "eslint": "8.37.0",
    "file-saver": "^2.0.5",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "5.5.0",
    "image-minimizer-webpack-plugin": "3.8.2",
    "jsonc-parser": "^3.0.0",
    "prettier": "2.8.7",
    "raw-loader": "^4.0.2",
    "rimraf": "^4.4.1",
    "screen-orientation": "^1.0.3",
    "sharp": "^0.32.0",
    "terser-webpack-plugin": "5.3.7",
    "tweedle.js": "^2.1.0",
    "typescript": "^4.9.5",
    "webpack": "5.77.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.2",
    "webpack-free-tex-packer": "https://github.com/miltoncandelero/webpack-free-tex-packer",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "6.5.4"
  },
  "dependencies": {
